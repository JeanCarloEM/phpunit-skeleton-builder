{
	"name": "jeancarloem/phpunit-skeleton-builder",
	"description": "Tool that can generate skeleton test classes from production code classes and vice versa",
	"keywords": ["phpunit", "xunit", "testing", "skeleton"],
	"homepage": "https://github.com/JeanCarloEM/phpunit-skeleton-builder",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Sebastian Bergmann",
			"email": "sebastian@phpunit.de"			
		},
		{
			"name": "Jean Carlo de Elias Moreira",
			"homepage": "https://jeancarloem.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/JeanCarloEM/phpunit-skeleton-builder/issues"
	},
	"require": {
		"php": ">=6.3.0",
		"phpunit/php-text-template": ">=1.2",
		"sebastian/version": ">=1.0|>=2.0",
		"symfony/console": ">=2.4"
	},
	"require-dev": {
		"phpunit/phpunit": ">=4.0",
		"mikey179/vfsstream": ">=1.2",
		"humbug/box": ">=3.0.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"bin": [
		"phpunit-skelgen"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	}
}